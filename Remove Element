class Solution {
public:
    int removeElement(vector<int>& nums, int val) 
    {
        int i = 0;
        int count = 0;

        while(i < nums.size())
        {
            if(nums[i] != val)
            {
                nums[count++] = nums[i];
            }
            i++;
        }


        return count;
    }
};
